uint16_t rgb888torgb565(long rgb888Pixel)
{
    uint8_t blue  = rgb888Pixel & 0b000000000000000011111111;
    uint8_t green = (rgb888Pixel >> 8) & 0b000000000000000011111111;
    uint8_t red   = (rgb888Pixel >> 16) & 0b000000000000000011111111;
    
    long b = map(blue, 0, 255, 0, 31);
    long g = map(green, 0, 255, 0, 2047-31);
    long r = map(red, 0, 255, 0, 65535-2047);
    
//    Serial.println(long (rgb888Pixel));
//    Serial.println(blue);
//    Serial.println(green);
//    Serial.println(red);
//    Serial.println(b);
//    Serial.println(g);
//    Serial.println(r);
//    Serial.println(uint16_t(r+g+b));
//    Serial.println();

    return (uint16_t(r+g+b));
}

void convertir(long bmp[]){
  for (int i=0; i<=15; i++) {
    for (int j=0; j<=15; j++) {
      rgb888torgb565(bmp[i*16+j]);
      Serial.print(rgb888torgb565(bmp[i*16+j]));
      Serial.print(", ");
    }
  }
}
